{
  "manifest_version": 2,
  "name": "Random Rails API tab",
  "version": "0.3.2",
  "description": "Open random Ruby on Rails API document on new tab",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "minimum_chrome_version": "58",
  "homepage_url": "http://github.com/uu1t/random-rails-api-tab",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["storage"]
}
